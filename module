# -*- mode: snippet -*-
# name: module
# contributor: Simon Unge
# key: module
# --
module ${1:`(replace-regexp-in-string "\.yang" "" (buffer-name))`} {
  namespace "${2:`(progn
                    (require 'json)
                    (if (file-exists-p "~/.yangsnippetrc")
                        (cdr (assq 'namespace (json-read-file "~/.yangsnippetrc")))))`$1}";
  prefix "${3:$1}";
  $0
}
